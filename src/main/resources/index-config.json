    "settings": {
        "index": {
            "analysis": {
                "analyzer": {
                    "german_analyzer": {
                        "filter": [
                            "lowercase",
                            "stop",
                            "snowball"
                        ],
                        "tokenizer": "standard",
                        "type": "custom"
                    },
                    "id_analyzer": {
                        "filter": [
                            "lowercase"
                        ],
                        "tokenizer": "keyword"
                    },
                    "ngram_analyzer": {
                        "filter": [
                            "lowercase",
                            "stop",
                            "ngram",
                            "snowball"
                        ],
                        "tokenizer": "letter",
                        "type": "custom"
                    }
                },
                "filter": {
                    "ngram": {
                        "max_gram": 40,
                        "min_gram": 1,
                        "type": "edgeNGram"
                    },
                    "snowball": {
                        "language": "German2",
                        "type": "snowball"
                    }
                }
            }
        }
    }
